    rdfs:label "Cali"@es ;
    dc:identifier "Cali" .

geo1:city_medellín a geo1:City ;
    rdfs:label "Medellín"@es ;
    dc:identifier "Medellín" .

geo1:dept_santander a geo1:Department ;
    rdfs:label "Santander"@es ;
    dc:identifier "Santander" .

geo1:locatedIn a owl:ObjectProperty ;
    rdfs:label "ubicada en"@es ;
    rdfs:domain univ:University ;
    rdfs:range geo1:Department .

geo1:originFrom a owl:ObjectProperty ;
    rdfs:label "originario de"@es ;
    rdfs:domain univ:Student ;
    rdfs:range geo1:City .

univ:age a owl:DatatypeProperty ;
    rdfs:label "edad"@es ;
    rdfs:domain univ:Student ;
    rdfs:range xsd:integer .

univ:appliesTo a owl:ObjectProperty ;
    rdfs:label "aplica a"@es ;
    rdfs:domain univ:Student ;
    rdfs:range univ:University .

univ:gender a owl:DatatypeProperty ;
    rdfs:label "género"@es ;
    rdfs:domain univ:Student ;
    rdfs:range xsd:string .

univ:hasInternationalAgreement a owl:DatatypeProperty ;
    rdfs:label "tiene convenio internacional"@es ;
    rdfs:domain univ:University ;
    rdfs:range xsd:boolean .

univ:hasScholarship a owl:DatatypeProperty ;
    rdfs:label "tiene beca disponible"@es ;
    rdfs:domain univ:University ;
    rdfs:range xsd:boolean .

univ:hasType a owl:ObjectProperty ;
    rdfs:label "tiene tipo"@es ;
    rdfs:domain univ:University .

univ:isAccredited a owl:DatatypeProperty ;
    rdfs:label "está acreditada"@es ;
    rdfs:domain univ:University ;
    rdfs:range xsd:boolean .

univ:nationalRanking a owl:DatatypeProperty ;
    rdfs:label "ranking nacional"@es ;
    rdfs:domain univ:University ;
    rdfs:range xsd:integer .

univ:socioeconomicStratum a owl:DatatypeProperty ;
    rdfs:label "estrato socioeconómico"@es ;
    rdfs:domain univ:Student ;
    rdfs:range xsd:integer .

