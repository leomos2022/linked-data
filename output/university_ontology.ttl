geo1:dept_antioquia a geo1:Department ;
    rdfs:label "Antioquia"@es ;
    dc:identifier "Antioquia" .

geo1:dept_atlántico a geo1:Department ;
    rdfs:label "Atlántico"@es ;
    dc:identifier "Atlántico" .

geo1:dept_valle_del_cauca a geo1:Department ;
    rdfs:label "Valle del Cauca"@es ;
