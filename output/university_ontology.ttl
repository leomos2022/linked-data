behavior:AcademicDecision a owl:Class ;
    rdfs:label "Decisión Académica"@es ;
    rdfs:comment "Elección realizada por un estudiante respecto a su educación"@es .

edu:AcademicProgram a owl:Class ;
    rdfs:label "Programa Académico"@es ;
    rdfs:comment "Conjunto estructurado de actividades académicas"@es .

geo1:dept_cundinamarca a geo1:Department ;
    rdfs:label "Cundinamarca"@es ;
    dc:identifier "Cundinamarca" .

geo1:City a owl:Class ;
    rdfs:label "Ciudad"@es ;
    rdfs:comment "Centro urbano de población"@es ;
    rdfs:subClassOf schema1:Place .

geo1:Department a owl:Class ;
    rdfs:label "Departamento"@es ;
    rdfs:comment "División administrativa territorial en Colombia"@es ;
    rdfs:subClassOf schema1:Place .

edu:KnowledgeArea a owl:Class ;
    rdfs:label "Área de Conocimiento"@es ;
    rdfs:comment "Campo específico del conocimiento"@es .

univ:Student a owl:Class ;
    rdfs:label "Estudiante"@es ;
    rdfs:comment "Persona que estudia en una institución educativa"@es ;
    rdfs:subClassOf foaf:Person .

univ:University a owl:Class ;
    rdfs:label "Universidad"@es ;
    rdfs:comment "Institución de educación superior"@es ;
    rdfs:subClassOf schema1:EducationalOrganization .

