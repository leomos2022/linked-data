@prefix behavior: <http://example.org/behavior/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix edu: <http://example.org/education/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo1: <http://example.org/geography/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix univ: <http://example.org/university/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

behavior:BehaviorPattern a owl:Class ;
    rdfs:label "Patrón de Comportamiento"@es ;
    rdfs:comment "Conjunto de decisiones y preferencias observables"@es .

behavior:finalDecision a owl:DatatypeProperty ;
    rdfs:label "decisión final"@es ;
    rdfs:domain behavior:AcademicDecision ;
    rdfs:range xsd:boolean .

behavior:makes a owl:ObjectProperty ;
    rdfs:label "realiza"@es ;
    rdfs:domain univ:Student ;
    rdfs:range behavior:AcademicDecision .

edu:area_administración a edu:KnowledgeArea ;
    rdfs:label "Administración"@es ;
    dc:identifier "Administración" .

edu:area_artes a edu:KnowledgeArea ;
    rdfs:label "Artes"@es ;
    dc:identifier "Artes" .

edu:area_ciencias_sociales a edu:KnowledgeArea ;
    rdfs:label "Ciencias Sociales"@es ;
    dc:identifier "Ciencias Sociales" .

edu:area_ingeniería a edu:KnowledgeArea ;
    rdfs:label "Ingeniería"@es ;
    dc:identifier "Ingeniería" .

edu:area_salud a edu:KnowledgeArea ;
    rdfs:label "Salud"@es ;
    dc:identifier "Salud" .

edu:belongsToArea a owl:ObjectProperty ;
    rdfs:label "pertenece al área"@es ;
    rdfs:domain edu:AcademicProgram ;
    rdfs:range edu:KnowledgeArea .

edu:prefersArea a owl:ObjectProperty ;
    rdfs:label "prefiere área"@es ;
    rdfs:domain univ:Student ;
    rdfs:range edu:KnowledgeArea .

edu:programModality a owl:DatatypeProperty ;
    rdfs:label "modalidad del programa"@es ;
    rdfs:domain edu:AcademicProgram ;
    rdfs:range xsd:string .

edu:saber11Score a owl:DatatypeProperty ;
    rdfs:label "puntaje Saber 11"@es ;
    rdfs:domain univ:Student ;
    rdfs:range xsd:decimal .

geo1:city_barranquilla a geo1:City ;
    rdfs:label "Barranquilla"@es ;
    dc:identifier "Barranquilla" .

geo1:city_bogotá a geo1:City ;
    rdfs:label "Bogotá"@es ;
    dc:identifier "Bogotá" .

geo1:city_bucaramanga a geo1:City ;
    rdfs:label "Bucaramanga"@es ;
    dc:identifier "Bucaramanga" .

